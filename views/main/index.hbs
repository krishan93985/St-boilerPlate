<script type="text/javascript">
    function validateGit(){
        let gitFname = $('#gitFname').val();
        let gitLname = $('#gitLname').val();
        let gitSubject = $('#gitSubject').val();
        let gitEmail = $('#gitEmail').val();
        let gitDesc = $('#gitDesc').val();

        if(gitFname.length === 0 || gitLname.length === 0 || gitSubject === 0 || gitEmail.length === 0 || gitDesc.length === 0){
            alert("Please fill in all Details");
            return false;
        }
        return true;
    }
</script>


