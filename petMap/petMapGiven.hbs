<div class="row" style="margin-top: 1rem;">
    <div class="col-lg-6 deskCard" style="padding: 2rem;">
        <h4>
            <b>Pricing and Packages</b>
        </h4>
        <div class="col-lg-12 col-11 col-md-10 my-3 PPmain coworkingMain">
            <div class="container-fluid px-0">
                <div class="row collapseTrigger uncollapsed pt-3 pb-2 py-md-4 px-2">
                    <div class="col-12 col-md-3 d-flex align-items-center packageTitle">{{space.priceStructure.pet.[0].petType}}</div>
                    <div class="col-12 col-md-6 d-flex align-items-center justify-content-start justify-content-md-center">Starting from {{space.priceStructure.pet.[0].price}} / {{space.priceStructure.pet.[0].billingInfo.billingDuration }} {{ space.priceStructure.pet.[0].billingInfo.billingCycle}}</div>
                    <div class="col-12 col-md-3 d-flex align-items-center justify-content-end collapseIcon uncollapsed"><i class="fas fa-chevron-up"></i></div>
                </div>
                <div class="container-fluid collapseTarget">
                    <hr class="mt-0">
                    <div class="row">
                        <div class="col-12 col-md-4 px-2 packageImg">
                            <img class="imageCard" src="{{space.priceStructure.pet.[0].unitImages}}" alt="">
                        </div>
                        <div class="col-12 col-md-8 py-0 d-flex align-items-center">
                            <div class="container-fluid px-0 py-3 px-md-3 py-md-2">
                                <div class="row">
                                    <div class="col-12 py-1">
                                        <h5>{{space.priceStructure.pet.[0].title}}</h5>
                                    </div>
                                    <div class="col-12 py-1">
                                        <h6>Description</h6>
                                        <p>{{space.priceStructure.pet.[0].description}}</p>
                                    </div>
                                    <div class="col-12 py-1">
                                        <label for="seatCapacity">Choose seat capacity:</label>
                                        <select name="seatCapacity" id="seatCapacity">
                                            <option value="20">{{space.priceStructure.pet.[0].seatCapacity}}</option>
                                            {{!-- <option value="40">40</option> --}}
                                        </select>
                                    </div>
                                    <div class="col-12 py-1">
                                        <span>Select duration: </span><div class="duration">{{space.priceStructure.pet.[0].price}} / {{space.priceStructure.pet.[0].billingInfo.billingDuration }} {{ space.priceStructure.pet.[0].billingInfo.billingCycle}}</div><div
                                            class="duration">{{space.priceStructure.pet.[0].price}} / {{space.priceStructure.pet.[0].billingInfo.billingDuration }} {{ space.priceStructure.pet.[0].billingInfo.billingCycle}}</div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <hr>
                    <div class="row d-flex align-items-center justify-content-between pb-3">
                        <button class="enquireBtn ml-3">Enquire</button>
                        <span class="price mr-3">₹<span class="">{{space.priceStructure.pet.[0].price}}</span> / {{space.priceStructure.pet.[0].billingInfo.billingDuration }} {{ space.priceStructure.pet.[0].billingInfo.billingCycle}}</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
